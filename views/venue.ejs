<!DOCTYPE html>
<html lang="en">

<head>
    <title></title>
    <%- include layout/materialize %>
</head>

<body align=center>
    <div class="row">

        <div class="col s2">
            <%- include layout/innerside %>
                <!-- Grey navigation panel -->
        </div>

        <div class="col s10">
            <!-- Teal page content  -->
            <%data.forEach(venue => {%>
                <div class="row">
                    <div class="col s12 m10">
                        <div class="card">
                            <div class="card-image">
                                <img src="<%= venue.photo %>">
                                <span class="card-title"><%= venue.name %></span>
                            </div>
                            <div class="card-content">
                                <p><%= venue.address %></p>
                            </div>
                            <div class="card-action">
                                <a href="#">Book</a>
                            </div>
                        </div>
                    </div>
                </div>
                <%})%>
        </div>

    </div>
</body>

</html>